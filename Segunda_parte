public class Main
{
    int puerta;


    public static void main(String[] args) {
		Coche unCoche = new Coche(2);
        System.out.println(unCoche.agregarPuerta());
    }
}
/* Creamos la clase Coche. */
class Coche{
    int puerta;


    public Coche(int puerta){
        this.puerta = puerta;

    }


    public int agregarPuerta(){
        return puerta = puerta + 1;
    }
}
